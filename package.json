{
    "name": "ttcss",
    "version": "1.0.0",
    "description": "帮助开发者快速编写css代码的工具库",
    "main": "index.js",
    "scripts": {
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "deploy": "bash deploy.sh",
        "example:dev": "cd example && yarn serve",
        "compilesass": "cd package && sass ttcss.scss ttcss.css",
        "compileless": "cd package && lessc ttcss.less ttcss.css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WangXingWeb/ttcss.git"
    },
    "keywords": [
        "css",
        "sass",
        "less"
    ],
    "author": "manjuan",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/WangXingWeb/ttcss/issues"
    },
    "homepage": "https://ttcss.iwangxing.cn",
    "devDependencies": {
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-medium-zoom": "^1.8.2",
        "@vuepress/plugin-pwa": "^1.8.2",
        "less": "^4.1.2",
        "vuepress": "^1.8.2",
        "vuepress-plugin-auto-sidebar": "^2.3.2"
    },
    "dependencies": {
        "moment": "^2.29.1"
    }
}
